	PlayerTextDrawSetProportional(playerid, PlayerTextdraws[playerid][ptextdraw_GAMEMODE_BG], 1);
	PlayerTextDrawSetSelectable(playerid, PlayerTextdraws[playerid][ptextdraw_GAMEMODE_BG], 0);
	return 1;
}

DestroyPlayerTextDraws(playerid)
{
	for(new i; i < sizeof(PlayerTextdraws[]); i++)
	{
		if(PlayerText:PlayerTextdraws[playerid][Player_Textdraws_Enum:i] == PlayerText:INVALID_TEXT_DRAW) continue;
		PlayerTextDrawDestroy(playerid, PlayerText:PlayerTextdraws[playerid][Player_Textdraws_Enum:i]);
		PlayerTextdraws[playerid][Player_Textdraws_Enum:i] = _:INVALID_TEXT_DRAW;
	}
	return 1;
}